-- Step 1: Create a temporary table to store unique records
CREATE TEMP TABLE mcc_unique AS
SELECT DISTINCT ON (code) *
FROM mcc
ORDER BY code, id;

-- Step 2: Delete all records from the original table
DELETE FROM mcc;

-- Step 3: Insert the unique records back into the original table
INSERT INTO mcc
SELECT * FROM mcc_unique;

-- Step 4: Drop the temporary table
DROP TABLE mcc_unique;
